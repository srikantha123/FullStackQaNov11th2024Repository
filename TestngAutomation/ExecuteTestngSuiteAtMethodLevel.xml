<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Users and Employee">
    <test name="Unit Testcases">
        <classes>
            <class name="com.sgtesting.users.Users">
                <methods>
                    <include name="createUser"></include>
                </methods>
            </class>
            <class name="com.sgtesting.employee.Employee">
                <methods>
                    <exclude name="modifyEmployee"></exclude>
                </methods>
            </class>
        </classes>
    </test>
</suite>